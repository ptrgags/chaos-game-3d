{
    "id": "no_going_back",
    "name": "No Going Back",
    "description": "grid_3d, but this time use a no-backtracking chooser to see if we get longer paths",
    "algorithm": "chaos_sets",
    "plotter": {
        "radius": 25
    },
    "initial_set": {
        "type": "box",
        "center": [0, 0, 0],
        "dims": [1, 1, 1],
        "color": [0, 1, 1],
        "num_points": 50
    },
    "initial_set_copies": 20,
    "iters": 100,
    "ifs": {
        "chooser": "no_backtracking",
        "xforms": [
            ["translate",  2,  0,  0],
            ["translate", -2,  0,  0],
            ["translate",  0,  2,  0],
            ["translate",  0, -2,  0],
            ["translate",  0,  0,  2],
            ["translate",  0,  0, -2]
        ]
    }
}
